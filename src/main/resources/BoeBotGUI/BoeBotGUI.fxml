      </Button>
      <Button layoutX="638.0" layoutY="266.0" mnemonicParsing="false" onAction="#onBuzzerClick" prefHeight="100.0" prefWidth="100.0" text="Buzzer">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="738.0" layoutY="266.0" mnemonicParsing="false" onAction="#onLightsClick" prefHeight="100.0" prefWidth="100.0" text="Lights">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="8.0" layoutY="8.0" mnemonicParsing="false" onAction="#switchToMap" text="Switch to map" />
